<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="294.0" prefWidth="370.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="exerciciotelas.classesobjetostelas.objetosPraDepois.NotebookController">

    <Label layoutX="50.0" layoutY="50.0" text="Modelo:" />
    <TextField fx:id="txtModelo" layoutX="120.0" layoutY="50.0" promptText="Digite o modelo do notebook" />

    <Label layoutX="50.0" layoutY="100.0" text="Música:" />
    <TextField fx:id="txtMusica" layoutX="120.0" layoutY="100.0" promptText="Digite a música para tocar" />

    <Button fx:id="btnLigarDesligar" layoutX="50.0" layoutY="150.0" onAction="#ligarDesligar" text="Ligar/Desligar" />
    <Button fx:id="btnAbrirFechar" layoutX="150.0" layoutY="150.0" onAction="#abrirFechar" text="Abrir/Fechar" />
    <Button fx:id="btnTocarMusica" layoutX="250.0" layoutY="150.0" onAction="#tocarMusica" text="Tocar Música" />

    <Label fx:id="lblResultado" layoutX="50.0" layoutY="200.0" text="Resultado das ações" />

</AnchorPane>
